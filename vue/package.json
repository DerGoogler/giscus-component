{
  "name": "@giscus/vue",
  "version": "2.0.0-beta.3",
  "main": "dist/giscus.cjs.js",
  "module": "dist/giscus.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/giscus.cjs.js",
      "import": "./dist/giscus.es.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --emitDeclarationOnly --declaration --outDir dist/types && mv dist/types/lib/* dist/ && rm -rf dist/types",
    "preview": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier src --check",
    "format:fix": "prettier src --write"
  },
  "dependencies": {
    "giscus": "^0.1.1"
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.5.0",
    "prettier": "^2.6.1",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  }
}
